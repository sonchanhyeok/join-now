<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MainMapper">
 
   <select id="selectList" resultType="PostDTO">
	    select *
		from post
   </select>
   
   <select id="totalCount" resultType="int">
	     select count(*)
	     from post
   </select>
   
</mapper>


